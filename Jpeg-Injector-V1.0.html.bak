<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>Jpeg-Injector V1.0</title>
<link rel="icon" href="win95_logo.png" type="image/png">
<style>
body {
    font-family: 'MS Sans Serif', 'Segoe UI', Tahoma, sans-serif;
    margin: 2rem;
    background-color: #008080; /* Classic Windows 95 teal */
    color: #000;
    background-image: url('data:image/png;base64,');
}

.desktop-icon {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 70px;
    margin-bottom: 20px;
    cursor: pointer;
}

.desktop-icon img {
    width: 32px;
    height: 32px;
    margin-bottom: 5px;
    border: none;
    background: transparent;
}

.desktop-icon span {
    color: white;
    text-align: center;
    font-size: 11px;
    text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.7);
}

.desktop-icons {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    position: absolute;
    top: 10px;
    left: 10px;
}

.window {
    background-color: #c0c0c0;
    border: 2px solid;
    border-color: #ffffff #808080 #808080 #ffffff;
    box-shadow: 2px 2px 0 0 #000;
    padding: 2px;
    margin: 50px auto;
    max-width: 800px;
    position: relative;
}

.title-bar {
    background: linear-gradient(to right, #000080, #1084d0);
    color: white;
    padding: 3px 5px;
    font-weight: bold;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.title-bar-controls {
    display: flex;
}

.title-bar-controls button {
    width: 16px;
    height: 14px;
    border: 1px solid #fff;
    background-color: #c0c0c0;
    margin-left: 2px;
    font-size: 9px;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0;
}

.title-bar-text {
    font-weight: bold;
    color: white;
    text-shadow: 1px 1px #000;
}

.window-body {
    padding: 10px;
    background-color: #c0c0c0;
}

#drop {
    width: 240px;
    height: 80px;
    border: 2px inset #fff;
    background-color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    color: #000;
    margin-bottom: 1rem;
    user-select: none;
}

img.preview-img {
    max-height: 300px;
    display: block;
    margin-bottom: 1rem;
    border: 1px solid #808080;
    background-color: #fff;
    padding: 1px;
}

textarea {
    width: 100%;
    height: 120px;
    margin-bottom: .9rem;
    font-family: 'Fixedsys', monospace;
    border: 2px inset #fff;
    background-color: #fff;
}

button {
    margin-right: .6rem;
    font-family: 'MS Sans Serif', sans-serif;
    color: #000;
    background-color: #c0c0c0;
    border: 2px solid;
    border-color: #ffffff #808080 #808080 #ffffff;
    padding: 4px 10px;
    min-width: 70px;
    box-shadow: 1px 1px 0 0 #000;
}

button:active {
    border-color: #808080 #ffffff #ffffff #808080;
    box-shadow: none;
}

/* Slider + badge --------------------------------------- */
#sliderWrap {
    position: relative;
    margin-bottom: 2.5rem;
}

#sliderWrap input[type=range] {
    width: 100%;
    height: 20px;
    -webkit-appearance: none;
    background-color: #c0c0c0;
    border: 2px inset #fff;
}

#sliderWrap input[type=range]::-webkit-slider-thumb {
    -webkit-appearance: none;
    width: 20px;
    height: 20px;
    background: #c0c0c0;
    border: 2px outset #fff;
}

#posBadge {
    position: absolute;
    top: -1.8rem;
    left: 0;
    background: #000080;
    color: #fff;
    padding: 2px 6px;
    border-radius: 0;
    font-size: .75rem;
    white-space: nowrap;
    transform: translateX(-50%);
    pointer-events: none;
    border: 1px solid #fff;
}

/* Manual input ----------------------------------------- */
#posInput {
    width: 100%;
    padding: .3rem .4rem;
    margin: -1.6rem 0 2.3rem;
    box-sizing: border-box;
    font-family: 'Fixedsys', monospace;
    border: 2px inset #fff;
}

hr {
    margin: 2.5rem 0 1.5rem;
    border: 1px inset #fff;
    border-width: 2px 0 0 0;
}

#info {
    font-size: .9rem;
    color: #000;
    margin: .4rem 0 1rem;
    font-family: 'MS Sans Serif', sans-serif;
}

.status-bar {
    border-top: 2px solid;
    border-color: #808080 #ffffff #ffffff #808080;
    padding: 5px;
    font-size: 0.8rem;
    display: flex;
    align-items: center;
}

/* Download button styles */
.download-btn {
    display: flex;
    align-items: center;
    margin-top: 10px;
    margin-bottom: 10px;
    font-family: 'MS Sans Serif', sans-serif;
    color: #000;
    background-color: #c0c0c0;
    border: 2px solid;
    border-color: #ffffff #808080 #808080 #ffffff;
    padding: 6px 12px;
    box-shadow: 1px 1px 0 0 #000;
    cursor: pointer;
}

.download-btn:active {
    border-color: #808080 #ffffff #ffffff #808080;
    box-shadow: none;
}

.download-btn img {
    width: 16px;
    height: 16px;
    margin-right: 5px;
    border: none;
    background: transparent;
    padding: 0;
}

/* Windows 95 taskbar */
.taskbar {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 30px;
    background-color: #c0c0c0;
    border-top: 2px solid #fff;
    display: flex;
    align-items: center;
    z-index: 9999;
}

.start-button {
    display: flex;
    align-items: center;
    padding: 2px 5px;
    margin-left: 2px;
    background-color: #c0c0c0;
    border: 2px solid;
    border-color: #ffffff #808080 #808080 #ffffff;
    height: 22px;
}

.start-button:active {
    border-color: #808080 #ffffff #ffffff #808080;
}

.start-button img {
    width: 16px;
    height: 16px;
    margin-right: 4px;
    border: none;
    background: transparent;
}

.taskbar-time {
    margin-left: auto;
    margin-right: 10px;
    border: 2px inset #fff;
    padding: 2px 5px;
    font-size: 11px;
}

/* Windows 95 font face */
@font-face {
    font-family: 'MS Sans Serif';
    src: url('https://unpkg.com/95-font/dist/ms-sans-serif-regular.woff2') format('woff2');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'MS Sans Serif';
    src: url('https://unpkg.com/95-font/dist/ms-sans-serif-bold.woff2') format('woff2');
    font-weight: bold;
    font-style: normal;
}

@font-face {
    font-family: 'Fixedsys';
    src: url('https://unpkg.com/fixedsys-css/FSEX300.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
</style>
</head>
<body>

<div class="desktop-icons">
    <div class="desktop-icon" id="openApp">
        <img src="images/win95_logo.png" alt="Jpeg Injector">
        <span>Jpeg Injector V1.0</span>
    </div>
</div>

<div class="window" id="appWindow">
    <div class="title-bar">
        <div class="title-bar-text">Jpeg-Injector V1.0</div>
        <div class="title-bar-controls">
            <button aria-label="Minimize">_</button>
            <button aria-label="Maximize">□</button>
            <button aria-label="Close" id="closeButton">×</button>
        </div>
    </div>
    <div class="window-body">
        <h4>A Project In Development - Complex_Echo_5845</h4>

        <div id="drop">
            Drag & Drop JPEG (≤ 3 MB) <br>or Click to Browse…
            <input id="chooser" type="file" accept="image/jpeg" hidden>
        </div>

        <img id="preview" class="preview-img" hidden alt="Original preview">

        <textarea id="txt" placeholder="Type or paste text (≤ 3 000 000 chars)"></textarea>

        <div id="info">(No file loaded)</div>

        <div id="sliderWrap">
            <input type="range" id="pos" min="0" max="0" value="0" disabled>
            <span id="posBadge">0</span>
        </div>

        <input id="posInput" type="number" min="0" value="0" disabled
               placeholder="Byte offset…" title="Exact byte position">

        <button id="insert">Insert</button>
        <form style="display: inline-block;">
            <input type="submit" id="reset" value="Start Over" style="font-family: 'MS Sans Serif', sans-serif; color: black; background-color: #c0c0c0; border: 2px solid; border-color: #ffffff #808080 #808080 #ffffff; padding: 4px 10px; min-width: 70px; box-shadow: 1px 1px 0 0 #000; margin-right: .6rem; cursor: pointer;">
        </form>

        <hr>

        <div id="result-container">
            <img id="result" class="preview-img" hidden alt="Modified preview">
            <button id="download-button" style="display:none;" class="download-btn">
                <img src="data:image/png;base64," style="width: 12px; height: 12px; margin-right: 5px;">
                Download Modified Image
            </button>
        </div>
    </div>
    <div class="status-bar">
        Ready
    </div>
</div>

<div class="taskbar">
    <button class="start-button" id="startButton">
        <img src="../images/win95_logo.png" alt="Start">
        <span>Start</span>
    </button>
    <div class="taskbar-time" id="taskbarTime">12:00 PM</div>
</div>

<script>
/**
 * Windows 95 Edition of Jpeg-Injector
 * 
 * CUSTOM SOUND INSTRUCTIONS:
 * To replace the Start Over button sound with your custom WAV:
 * 1. Find the line with "YOUR_BASE64_WAV_SOUND_HERE" in the Sound Effects section
 * 2. Replace that placeholder with your base64-encoded WAV sound data
 */
(() => {
/* ---------- DOM shortcuts ---------- */
const drop     = document.getElementById('drop');
const chooser  = document.getElementById('chooser');
const preview  = document.getElementById('preview');
const txt      = document.getElementById('txt');
const pos      = document.getElementById('pos');
const posInput = document.getElementById('posInput');
const badge    = document.getElementById('posBadge');
const info     = document.getElementById('info');
const insertBt = document.getElementById('insert');
const result   = document.getElementById('result');
const downloadBtn = document.getElementById('download-button');
const appWindow = document.getElementById('appWindow');
const closeButton = document.getElementById('closeButton');
const openApp = document.getElementById('openApp');
const taskbarTime = document.getElementById('taskbarTime');

/* ---------- Windows 95 UI functionality ---------- */
// Window controls
closeButton.addEventListener('click', () => {
    appWindow.style.display = 'none';
});

openApp.addEventListener('click', () => {
    appWindow.style.display = 'block';
});

// Make window draggable
let isDragging = false;
let offsetX, offsetY;

document.querySelector('.title-bar').addEventListener('mousedown', (e) => {
    isDragging = true;
    offsetX = e.clientX - appWindow.getBoundingClientRect().left;
    offsetY = e.clientY - appWindow.getBoundingClientRect().top;
});

document.addEventListener('mousemove', (e) => {
    if (!isDragging) return;
    
    appWindow.style.left = (e.clientX - offsetX) + 'px';
    appWindow.style.top = (e.clientY - offsetY) + 'px';
});

document.addEventListener('mouseup', () => {
    isDragging = false;
});

// Update clock
function updateClock() {
    const now = new Date();
    let hours = now.getHours();
    const minutes = now.getMinutes().toString().padStart(2, '0');
    const ampm = hours >= 12 ? 'PM' : 'AM';
    
    hours = hours % 12;
    hours = hours ? hours : 12; // Convert 0 to 12
    
    taskbarTime.textContent = `${hours}:${minutes} ${ampm}`;
}

// Update clock every minute
updateClock();
setInterval(updateClock, 60000);

/* ---------- State ---------- */
let origBytes = null;          // Uint8Array of original JPEG
let objURL    = null;          // object-URL for preview
let fileName = "";             // Original filename

/* ---------- Utilities ---------- */
const revokeURL = () => { if (objURL){ URL.revokeObjectURL(objURL); objURL = null; } };

function updateBadgeAndInput(){
    const pct = (pos.value - pos.min) / (pos.max - pos.min || 1);
    badge.style.left = `${pct * pos.getBoundingClientRect().width}px`;
    badge.textContent = pos.value;
    posInput.value = pos.value;
}

function clamp(val, lo, hi){
    if (isNaN(val)) return lo;
    return Math.min(Math.max(val, lo), hi);
}

/* ---------- File loading (shared by DnD & chooser) ---------- */
function showWin95Error(message) {
    const errorDiv = document.createElement('div');
    errorDiv.className = 'win95-error';
    errorDiv.style.position = 'fixed';
    errorDiv.style.top = '50%';
    errorDiv.style.left = '50%';
    errorDiv.style.transform = 'translate(-50%, -50%)';
    errorDiv.style.zIndex = '9999';
    
    // Create error dialog content
    errorDiv.innerHTML = `
        <div class="window" style="margin:0; min-width:300px;">
            <div class="title-bar">
                <div class="title-bar-text">Error</div>
                <div class="title-bar-controls">
                    <button aria-label="Close" class="error-close">×</button>
                </div>
            </div>
            <div class="window-body" style="display:flex; padding:15px;">
                <img src="images/win95_error.png" style="width:40px; height:40px; margin-right:10px; border:none; background:transparent;">
                <p style="margin:5px 0;">${message}</p>
            </div>
            <div style="text-align:center; padding:10px;">
                <button class="error-ok">OK</button>
            </div>
        </div>
    `;
    
    document.body.appendChild(errorDiv);
    
    // Add event listeners for buttons
    errorDiv.querySelector('.error-close').addEventListener('click', () => {
        document.body.removeChild(errorDiv);
    });
    
    errorDiv.querySelector('.error-ok').addEventListener('click', () => {
        document.body.removeChild(errorDiv);
    });
    
    // Play Windows 95 error sound
    const errorSound = new Audio('data:audio/mp3;base64,');
    errorSound.play();
}

function loadFile(file){
    if (file.type !== 'image/jpeg'){ 
        showWin95Error('Only JPEG files are supported.');
        return; 
    }
    if (file.size > 3*1024*1024){
        showWin95Error('File exceeds 3 MB limit.');
        return; 
    }

    // Store the original filename
    fileName = file.name;
    
    // Hide download button when new file is loaded
    downloadBtn.style.display = 'none';

    revokeURL();
    objURL       = URL.createObjectURL(file);
    preview.src  = objURL;
    preview.hidden = false;
    result.hidden  = true;

    const reader = new FileReader();
    reader.onload = e=>{
        origBytes = new Uint8Array(e.target.result);

        pos.max         = origBytes.length;     // allow EOF insert
        pos.disabled    = false;
        pos.value       = 0;

        posInput.max    = origBytes.length;
        posInput.disabled = false;
        posInput.value  = 0;

        updateBadgeAndInput();

        info.textContent =
          `File size: ${origBytes.length.toLocaleString()} bytes — slider 0…${origBytes.length}`;
          
        // Update status bar
        document.querySelector('.status-bar').textContent = 'File loaded successfully';
    };
    reader.readAsArrayBuffer(file);
}

/* ---------- Drag-and-drop ---------- */
['dragenter','dragover'].forEach(evt =>
    drop.addEventListener(evt, e=>{ e.preventDefault(); drop.style.borderColor='#000080'; })
);
['dragleave','drop'].forEach(evt =>
    drop.addEventListener(evt, e=>{ e.preventDefault(); drop.style.borderColor='#fff'; })
);
drop.addEventListener('drop', e=>{
    const f = e.dataTransfer.files[0];
    if (f) loadFile(f);
});

/* ---------- Click-to-browse ---------- */
drop.addEventListener('click', () => chooser.click());
chooser.addEventListener('change', () => {
    const f = chooser.files[0];
    if (f) loadFile(f);
});

/* ---------- Slider ↔ input synchronisation ---------- */
pos.addEventListener('input', updateBadgeAndInput);

posInput.addEventListener('input', () => {
    if (pos.disabled) return;
    const v = clamp(Number(posInput.value), Number(pos.min), Number(pos.max));
    pos.value = v;               // snaps slider
    updateBadgeAndInput();
});

/* ---------- Insert logic ---------- */
insertBt.addEventListener('click', ()=> {
    if (!origBytes){ 
        showWin95Error('Load a JPEG first before attempting to insert text.');
        return; 
    }
    const text = txt.value;
    if (!text){ 
        showWin95Error('Text box is empty. Please enter some text to inject.');
        return; 
    }
    if (text.length > 3_000_000){ 
        showWin95Error('Text exceeds 3 000 000 character limit.');
        return; 
    }

    const p = Number(pos.value);
    const textBytes = new Uint8Array(text.length);
    for (let i = 0; i < text.length; i++) textBytes[i] = text.charCodeAt(i) & 0xFF;

    const out = new Uint8Array(origBytes.length + textBytes.length);
    out.set(origBytes.subarray(0, p), 0);                 // pre-slice
    out.set(textBytes, p);                                // insertion
    out.set(origBytes.subarray(p), p + textBytes.length); // post-slice

    const blob   = new Blob([out], {type:'image/jpeg'});
    const modURL = URL.createObjectURL(blob);
    result.src   = modURL;
    result.hidden = false;
    
    // Show download button
    downloadBtn.style.display = 'flex';
    
    // Setup download filename based on original filename
    let modifiedFileName = fileName;
    if (fileName) {
        // Extract base name without extension
        const dotIndex = fileName.lastIndexOf('.');
        if (dotIndex > 0) {
            const baseName = fileName.substring(0, dotIndex);
            modifiedFileName = baseName + '_modified.jpg';
        } else {
            modifiedFileName = fileName + '_modified.jpg';
        }
    } else {
        modifiedFileName = 'modified_image.jpg';
    }
    
    // Set up download button
    downloadBtn.onclick = () => {
        const downloadLink = document.createElement('a');
        downloadLink.href = modURL;
        downloadLink.download = modifiedFileName;
        
        // Play Windows 95 sound when downloading
        playSound(startOverSound);
        
        // Trigger download
        document.body.appendChild(downloadLink);
        downloadLink.click();
        document.body.removeChild(downloadLink);
        
        // Update status bar
        document.querySelector('.status-bar').textContent = 'Image downloaded successfully';
    };
    
    // Play success sound
    playSound(insertSuccessSound);
    
    // Update status bar
    document.querySelector('.status-bar').textContent = 'Text successfully injected';
});

/* ---------- Sound Effects ---------- */
// Placeholder for your custom sound - replace this with your base64 WAV data
const startOverSound = 'data:audio/mp3;base64,';

// Windows 95 success sound (tada.wav) - you can replace this with your custom sound too
const insertSuccessSound = 'data:audio/mp3;base64,';

function playSound(soundData) {
    const audio = new Audio(soundData);
    audio.play();
}

/* ---------- Reset ---------- */
// Get the form and reset button
const resetForm = document.querySelector('form');
const resetBtn = document.getElementById('reset');

// Add event listener to play sound before form submission
resetForm.addEventListener('submit', (e) => {
    // Play the start over sound
    playSound(startOverSound);
    
    // Small delay to allow the sound to start playing before page refreshes
    // No need to prevent default as we want the form to submit and refresh
});
})();
</script>
</body>
</html>